{
  "name": "geo-data-viewer",
  "displayName": "Geo Data Viewer",
  "description": "🗺️ Geo Data Viewer w/0 Py 🐍 || pyWidgets || pandas 🐼 ||  @reactjs  ⚛️ required to gen. some snazzy maps 🗺️ with keplerGL ...",
  "version": "1.4.0",
  "icon": "images/map.png",
  "publisher": "RandomFractalsInc",
  "author": "Taras Novak a.k.a. dataPixy devTools maker :)",
  "contributors": [
    "Taras Novak"
  ],
  "license": "Apache-2.0",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/RandomFractals/geo-data-viewer"
  },
  "bugs": "https://github.com/RandomFractals/geo-data-viewer/issues",
  "homepage": "https://github.com/RandomFractals/geo-data-viewer/README.md",
  "keywords": [
    "geo data analytics",
    "geo data viewer",
    "transform geo data",
    "map view 🗺️",
    "keplerGL",
    "deckGL",
    "mapbox",
    "geojson",
    "topojson",
    "GIS",
    "GISPad 📘",
    "map gist",
    "preview",
    "dataViz",
    "dev tools"
  ],
  "galleryBanner": {
    "color": "#333",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.42.0",
    "node": ">=10.2.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onCommand:map.view",
    "onCommand:map.view.url",
    "onCommand:map.gallery",
    "onWebviewPanel:map.view",
    "onLanguage:csv",
    "onLanguage:json",
    "onLanguage:geojson",
    "onLanguage:topojson",
    "onLanguage:kgl"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "csv",
        "extensions": [
          ".csv"
        ],
        "aliases": [
          "CSV"
        ]
      },
      {
        "id": "json",
        "extensions": [
          ".json",
          ".geojson",
          ".kgl",
          ".topojson"
        ],
        "aliases": [
          "JSON",
          "GeoJSON",
          "KeplerGL JSON",
          "Topology JSON"
        ],
        "configuration": "./json-language-configuration.json"
      }
    ],
    "jsonValidation": [
      {
        "fileMatch": "*.geojson",
        "url": "https://geojson.org/schema/GeoJSON.json"
      }
    ],
    "commands": [
      {
        "command": "map.view",
        "title": "View Map",
        "category": "Geo",
        "icon": {
          "light": "./images/map.svg",
          "dark": "./images/map.svg"
        }
      },
      {
        "command": "map.view.url",
        "title": "View Map from URL",
        "category": "Geo"
      },
      {
        "command": "map.gallery",
        "title": "Map Gallery",
        "category": "Geo"
      }
    ],
    "keybindings": [
      {
        "command": "map.view.url",
        "key": "ctrl+alt+u"
      },
      {
        "command": "map.view",
        "key": "ctrl+alt+m"
      },
      {
        "command": "map.gallery",
        "key": "ctrl+alt+g"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "map.view",
          "when": "resourceFilename =~ /.*\\.(csv|json|geojson|kgl|topojson)/",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "command": "map.view",
          "when": "resourceFilename =~ /.*(.geo.json|.geojson|.kgl.json|keplergl.json|keplergl_time.json|.map.json|.topo.json|.topojson)/",
          "group": "navigation"
        }
      ],
      "editor/title/context": [
        {
          "command": "map.view",
          "when": "resourceFilename =~ /.*\\.(csv|json|geojson|kgl|topojson)/",
          "group": "navigation"
        }
      ]
    },
    "gistpad.viewers": [
      {
        "filenames": [
          ".geo.json",
          ".geojson",
          ".kgl.json",
          "keplergl.json",
          "keplergl_time.json",
          ".map.json",
          ".topo.json",
          ".topojson"
        ],
        "command": "map.view"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.6",
    "@types/superagent": "^4.1.7",
    "@types/vscode": "^1.42.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.0",
    "typescript": "^3.8.2",
    "tslint": "^6.0.0",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@mapbox/togeojson": "^0.16.0",
    "superagent": "^5.2.2",
    "xmldom": "^0.3.0"
  }
}
