{
  "name": "geo-data-viewer",
  "displayName": "Geo Data Viewer",
  "description": "Geo Data üó∫Ô∏è Viewer",
  "version": "1.0.0",
  "icon": "images/map.png",
  "publisher": "RandomFractalsInc",
  "author": "Taras Novak a.k.a. dataPixy devTools maker :)",
  "contributors": [
    "Taras Novak"
  ],
  "license": "Apache-2.0",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/RandomFractals/geo-data-viewer"
  },
  "bugs": "https://github.com/RandomFractals/geo-data-viewer/issues",
  "homepage": "https://github.com/RandomFractals/geo-data-viewer/README.md",
  "keywords": [
    "geo data analytics",
    "geo data viewer",
    "transform geo data",
    "map view",
    "geojson",
    "gis",
    "preview",
    "dataViz",
    "dev tools"
  ],
  "galleryBanner": {
    "color": "#333",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onCommand:view.map",
    "onWebviewPanel:map.view",
    "onLanguage:geojson"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "geojson",
        "extensions": [
          ".geojson"
        ],
        "aliases": [
          "GEOJSON"
        ]
      }
    ],
    "commands": [
      {
        "command": "view.map",
        "title": "View Map",
        "category": "Geo",
        "icon": {
          "light": "./images/map.svg",
          "dark": "./images/map.svg"
        }
      }
    ],
    "keybindings": [
      {
        "command": "view.map",
        "key": "ctrl+alt+m"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "view.map",
          "when": "resourceFilename =~ /.*\\.(geojson)/",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "command": "view.map",
          "when": "resourceFilename =~ /.*\\.(geojson)/",
          "group": "navigation"
        }
      ],
      "editor/title/context": [
        {
          "command": "view.map",
          "when": "resourceFilename =~ /.*\\.(geojson)/",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.2",
    "@types/vscode": "^1.41.0",
    "glob": "^7.1.6",
    "mocha": "^6.2.2",
    "typescript": "^3.7.4",
    "tslint": "^5.20.1",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {}
}
